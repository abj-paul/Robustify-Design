        	<uml:State name="EM"/>
    	</uml:Source>
    	<uml:Target>
        	<uml:State name="P1"/>
    	</uml:Target>
    	<uml:Effect name="password"/>
	</uml:Transition>
	<uml:Transition>
    	<uml:Source>
        	<uml:State name="P1"/>
    	</uml:Source>
    	<uml:Target>
        	<uml:State name="P2"/>
    	</uml:Target>
    	<uml:Effect name="select"/>
	</uml:Transition>
	<uml:Transition>
    	<uml:Source>
        	<uml:State name="P2"/>
    	</uml:Source>
    	<uml:Target>
        	<uml:State name="P3"/>
    	</uml:Target>
    	<uml:Effect name="vote"/>
	</uml:Transition>
	<uml:Transition>
    	<uml:Source>
        	<uml:State name="P2"/>
    	</uml:Source>
    	<uml:Target>
        	<uml:State name="P1"/>
    	</uml:Target>
    	<uml:Effect name="back"/>
	</uml:Transition>
	<uml:Transition>
    	<uml:Source>
        	<uml:State name="P3"/>
    	</uml:Source>
    	<uml:Target>
        	<uml:State name="EM"/>
    	</uml:Target>
    	<uml:Effect name="confirm"/>
	</uml:Transition>
	<uml:Transition>
    	<uml:Source>
        	<uml:State name="P3"/>
    	</uml:Source>
    	<uml:Target>
        	<uml:State name="P2"/>
    	</uml:Target>
    	<uml:Effect name="back"/>
	</uml:Transition>
</uml:Model>
