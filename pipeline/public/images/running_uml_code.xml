<?xml version="1.0" encoding="UTF-8"?>
<uml:Model xmlns:uml="http://www.omg.org/spec/UML/20090901">
<uml:State name="P2"/>
<uml:State name="P1"/>
<uml:State name="P3"/>
<uml:State name="EM"/>

	<uml:Transition>
